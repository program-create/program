<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.qfedu.mapper.MonthticketMapper" >
  <resultMap id="BaseResultMap" type="com.qfedu.pojo.Monthticket" >
    <id column="id" property="id" jdbcType="INTEGER" />
    <result column="bookid" property="bookid" jdbcType="INTERGER"></result>
    <result column="votetime" property="votetime" jdbcType="TIMESTAMP" />
    <result column="votenum" property="votenum" jdbcType="INTEGER" />
    <result column="uid" property="uid" jdbcType="INTEGER" />
  </resultMap>
  <sql id="Base_Column_List" >
    id, votetime, votenum, uid
  </sql>

<insert id="insertMonthticket" parameterType="com.qfedu.pojo.Monthticket">
  insert into t_monthticket <include refid="Base_Column_List" />
  values (null,now(),#{votenum},#{uid})
</insert>

</mapper>